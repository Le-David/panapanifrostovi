{
	"name": "panapanifrostovi",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "run-s build:*",
		"build:css": "npm run generate:css",
		"build:remix": "remix build",
		"dev": "run-p dev:*",
		"dev:css": "npm run generate:css:watch",
		"dev:remix": "remix dev",
		"start": "netlify dev",
		"generate:css": "sass app/:app/ --style=compressed",
		"generate:css:watch": "sass --watch app/:app/ --style=compressed",
		"typecheck": "tsc -b"
	},
	"dependencies": {
		"@netlify/functions": "^2.0.0",
		"@netlify/remix-adapter": "^2.0.0",
		"@netlify/remix-runtime": "^2.1.0",
		"@remix-run/node": "^2.6.0",
		"@remix-run/react": "^2.6.0",
		"@supabase/supabase-js": "^2.39.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@remix-run/css-bundle": "^2.6.0",
		"@remix-run/dev": "^2.6.0",
		"@remix-run/eslint-config": "^2.6.0",
		"@remix-run/serve": "^2.6.0",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.0",
		"eslint": "^8.27.0",
		"npm-run-all": "^4.1.5",
		"sass": "^1.70.0",
		"typescript": "^5.2.2"
	},
	"engines": {
		"node": ">=18"
	}
}
